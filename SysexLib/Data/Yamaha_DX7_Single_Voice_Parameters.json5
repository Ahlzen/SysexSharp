// OP 1-6 parameters
for (int op = 0; op < 6; op++)
{
    var offset = (5 - op) * 21; // OPs are stored in reverse order
    string prefix = name: "OP " + (op + 1) + " ";
    {offset: offset + 0, prefix + name: "EG Rate 1", minValue: 0, maxValue: 99},
    {offset: offset + 1, prefix + name: "EG Rate 2", minValue: 0, maxValue: 99},
    {offset: offset + 2, prefix + name: "EG Rate 3", minValue: 0, maxValue: 99},
    {offset: offset + 3, prefix + name: "EG Rate 4", minValue: 0, maxValue: 99},
    {offset: offset + 4, prefix + name: "EG Level 1", minValue: 0, maxValue: 99},
